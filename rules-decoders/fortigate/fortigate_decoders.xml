<!-- FortiOS 4.0 --> 
<decoder name="fortigate-firewall-4"> 
  <prematch>date=\S+ time=\.+ devname=\S+ device_id=FG\w+ </prematch> 
  <prematch>log_id=\d+ type=traffic subtype=other pri=\S+ status=</prematch> 
  <type>syslog</type> 
</decoder> 

<decoder name="fortigate-firewall-4"> 
  <parent>fortigate-firewall-4</parent> 

  <regex offset="after_parent">(\w+) \.+ </regex> 
  <regex>src=(\d+.\d+.\d+.\d+) \.+ src_port=(\d+) </regex> 
  <regex>dst=(\d+.\d+.\d+.\d+) \.+ dst_port=(\d+) </regex> 
  <regex>\.*service=\d+/(\w+) </regex> 
  <order>action,srcip,srcport,dstip,dstport,protocol</order> 
</decoder> 


